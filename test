curl -i -X GET http://api.con/api/slider/search/544120160812063306.jpeg

curl -i -X GET http://api.con/api/slider/1

-- POST Data --
curl -i -X POST -d '{"name_slider":"test.jpg"}' http://api.con/api/slider

-- PUT Data --
curl -i -X PUT -d '{"name_slider":"test1.jpg"}' http://api.con/api/slider/26

-- DELETE Data --
curl -i -X DELETE http://api.con/api/slider/2

================ END =============================================





=============== TEST API Career Hexaview =========================
-- GET Data --
curl -i -X GET http://api.con/api/career

curl -i -X GET http://api.con/api/career/search/web

curl -i -X GET http://api.con/api/career/1

-- POST Data --

curl -i -X POST -d '{"tittle_career":"test","descriptions_career":"test","responsibilities_career":"test","qualifications_career":"test","date_post":"19/9/2016","close_date":"test","location_career":"jakarta","image_career":"test.jpg"}' http://api.con/api/career

-- PUT Data
curl -i -X PUT -d '{"tittle_career":"test1","descriptions_career":"test1","responsibilities_career":"test1","qualifications_career":"test1","date_post":"29/9/2016","close_date":"test1","location_career":"jakarta1","image_career":"test1.jpg"}' http://api.con/api/career/26

-- DELETE Data --
curl -i -X DELETE http://api.con/api/career/2

================ END =============================================